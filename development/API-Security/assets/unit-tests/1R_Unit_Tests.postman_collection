								"header": [
									{
										"key": "Accept",
										"value": "application/ld+json; version=2.0.0-dev",
										"type": "text"
									}
								],
								"body": {
									"mode": "raw",
									"raw": ""
								},
								"url": {
									"raw": "{{baseUrl}}/logistics-objects/:logisticsObjectId",
									"host": [
										"{{baseUrl}}"
									],
